<launch>
<arg name="camera_topic_in" default="/realsense_img" />
	<arg name="camera_topic_out" default="/realsense_obj" />


    <node pkg="nodelet" type="nodelet" args="manager" name="realsense_nodelet_manager" output="screen" />

<node pkg="nodelet" type="nodelet" args="load realsense/realsense_nodelet realsense_nodelet_manager" name="real_nodelet" output="screen">
		<param name="camera_topic_in" value="$(arg camera_topic_in)" />
	</node>
</launch>